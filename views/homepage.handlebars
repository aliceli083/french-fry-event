<div class="row">
  <div class="col s12 ">
    <div class="card white">
      <div class="card-content black-text">
        <span class="card-title red-text text-accent-3">Upcoming Events</span>
        <img src={{images.0.image}} style="max-width: 50%"></img>
        <ul>
          {{#each eventsNEW as |event|}}
          <li>{{event.event_name}} | {{event.event_date}}</li>
          {{/each}}
        </ul>
      </div>
      <div class="card-action">
        <a class="blue-text text-lighten-1" href="#">View All</a>
      </div>
    </div>
    <div class="card white">
      <div class="card-content black-text">
        <span class="card-title red-text text-accent-3">All Events</span>
        <img src={{images.1.image}} style="max-width: 50%"></img>
        <ul>
          {{#each eventsHOST as |event|}}
          <li>{{event.event_name}} | {{event.event_date}}</li>
          {{/each}}
        </ul>
      </div>
      <div class="card-action">
        <a class="blue-text text-lighten-1" href="#">View All</a>
      </div>
    </div>
    <div class="card white">
      <div class="card-content black-text">
        <span class="card-title red-text text-accent-3">Past Events</span>
        <img src={{images.2.image}} style="max-width: 50%"></img>
        <ul>
          {{#each eventsOLD as |event|}}
          <li>{{event.event_name}} | {{event.event_date}}</li>
          {{/each}}
        </ul>
      </div>
      <div class="card-action">
        <a class="blue-text text-lighten-1" href="#">View All</a>
      </div>
    </div>
  </div>
</div>
</div>